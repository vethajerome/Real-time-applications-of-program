import java.util.*;
class Main 
{
    public static void main(String args[])
    {
        Scanner g=new Scanner(System.in);
        int current=g.nextInt();
        int sum=0;
        int n=g.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=g.nextInt();
            sum+=a[i];
            if(i==n-1)
            current=current-sum;
        }
        System.out.print(current);
    }
}
